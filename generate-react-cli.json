{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Enzyme",
  "component": {
      "store": {
          "customTemplates": {
              "component": "templates/store/TemplateName.tsx"
          },
          "path": "src/stores",
          "withLazy": false,
          "withStory": false,
          "withStyle": false,
          "withTest": false
      },
      "component": {
          "customTemplates": {
              "component": "templates/component/TemplateName.tsx",
              "style": "templates/component/TemplateName.scss"
          },
          "path": "src/components",
          "withLazy": false,
          "withStory": false,
          "withStyle": true,
          "withTest": false
      },
      "page": {
          "customTemplates": {
              "component": "templates/page/TemplateName.tsx",
              "style": "templates/page/TemplateName.scss"
          },
          "path": "src/pages",
          "withLazy": false,
          "withStory": false,
          "withStyle": true,
          "withTest": false
      },
      "default": {
          "path": "src/components",
          "withStyle": true,
          "withTest": false,
          "withStory": false,
          "withLazy": false
      }
  }
}
